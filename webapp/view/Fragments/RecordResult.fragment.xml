						<core:Icon src="sap-icon://accept" color="#037203" visible="{= ${RecordResultModel>Evaluation} === 'A' ? (${RecordResultModel>Status} !== '1' ? (${RecordResultModel>Status} !== '2' ? true : false) : false) : false}">
							<core:layoutData>
								<FlexItemData growFactor="1" />
							</core:layoutData>
						</core:Icon>
						<core:Icon src="sap-icon://decline" color="#a50404" visible="{=${RecordResultModel>Evaluation} === 'A' ? false : true}">
							<core:layoutData>
								<FlexItemData growFactor="1" />
							</core:layoutData>
						</core:Icon>
					</HBox>
			
<!-- Code Written By Hari Krishna on 05072024 Ends Here -->						

				</cells>
			</ColumnListItem>
		</items>
	</Table>
</core:FragmentDefinition>